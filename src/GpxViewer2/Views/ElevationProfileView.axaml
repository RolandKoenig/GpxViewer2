<ext:MvvmUserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:lvc="using:LiveChartsCore.SkiaSharpView.Avalonia"
             xmlns:ext="https://github.com/RolandK.AvaloniaExtensions"
             xmlns:local="clr-namespace:GpxViewer2.Views"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="GpxViewer2.Views.ElevationProfileView"
             x:DataType="local:ElevationProfileViewModel"
             d:DataContext="{x:Static local:ElevationProfileViewModel.EmptyViewModel}"
             ViewFor="{x:Type local:ElevationProfileViewModel}">
    <lvc:CartesianChart x:Name="CtrlChart" 
                        Series="{Binding Series}"
                        ZoomMode="None">
    </lvc:CartesianChart>
</ext:MvvmUserControl>
