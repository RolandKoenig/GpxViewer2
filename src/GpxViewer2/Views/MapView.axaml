<ext:MvvmUserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:mapsui="clr-namespace:Mapsui.UI.Avalonia;assembly=Mapsui.UI.Avalonia"
             xmlns:ext="https://github.com/RolandK.AvaloniaExtensions"
             xmlns:local="clr-namespace:GpxViewer2.Views"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="GpxViewer2.Views.MapView"
             d:DataContext="{x:Static local:MapViewModel.EmptyViewModel}"
             ViewFor="{x:Type local:MapViewModel}">
    <mapsui:MapControl x:Name="MapControl"
                       PointerPressed="OnMapControl_PointerPressed"
                       PointerReleased="OnMapControl_PointerReleased"
                       PointerMoved="OnMapControl_PointerMoved"/>
</ext:MvvmUserControl>
